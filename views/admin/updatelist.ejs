<% include('../layouts/header.ejs') %>
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>
<h2>Update User</h2>
<form action="" method="post">
    <input type="hidden" name="id" value="<%= userlist[0].id %>">
    <input type="text" name="uname" value="<%= userlist[0].uname %>" placeholder="Enter Name" required><br><br>
    <input type="email" name="uemail" value="<%= userlist[0].uemail %>" placeholder="Enter Email" required><br><br>
    <input type="text" name="umobile" value="<%= userlist[0].umobile %>" placeholder="Enter Mobile" required><br><br>
    <input type="submit" value="Update" class="btn btn-primary">

</form>

<% include('../layouts/footer.ejs') %> 